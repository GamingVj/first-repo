' AIM: To identify Whether the entered number is positive or negative.

Private Sub cmdcheck_Click ()
	Dim a As Integer
	a = txtone.Text
	If a>0 Then
	Msgbox("The number you entered is positive")
	Else if a<0 Then
	Msgbox("The number you entered is negative")
	Else
	Msgbox("You have entered zero which is neither positive or negative")
	Else if
End Sub